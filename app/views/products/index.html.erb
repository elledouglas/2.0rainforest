<h1>Products </h1>
<%= link_to("New Product", "/products/new") %>



<ul>

    <% @products.each do |product| %>
<li>
  <%= product.name %>
<%= product.description %>
<%=  product.price_in_cents %>
<%= link_to "Edit", "/products/#{product.id}/edit" %>
<%= link_to "Delete", "/products/:id" %>
</li>
  <% end %>
</ul>
